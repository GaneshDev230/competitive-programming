//https://codeforces.com/contest/903/problem/C
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    map<int ,int>mp;
    for(int i=0;i<n;i++)
    {
        int a;
        cin>>a;
        mp[a]++;
        
    }
   int max=0;
    for(auto c:mp)
    {
        if(max<c.second)
        {
            max=c.second;
        }
        
    }
    cout<<max;
    return 0;
    
}
