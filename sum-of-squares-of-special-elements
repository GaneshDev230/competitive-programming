//https://leetcode.com/problems/sum-of-squares-of-special-elements/
class Solution {
public:
    int sumOfSquares(vector<int>& nums) {
        int n=nums.size();
        int su=0;
        for(int i=0;i<n;i++)
        {
            if(n%(i+1)==0)
            {
               
               
                
                     su+=nums[i]*nums[i];
                
            }
        }
        return su;
        
    }
};
